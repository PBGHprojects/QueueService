{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "QueueService": {
    "Provider": "[PROVIDER]", // supported values: AzureStorageQueue, AzureServiceBus
    "ConnectionString": "[CONNECTION_STRING]",
    "QueueName": "[QUEUE_NAME]", // project assumes that queue exists
    "MessageType": "[MESSAGE_TYPE]" // can be any value
  }
}
